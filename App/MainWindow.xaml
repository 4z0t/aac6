<Window x:Class="App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:App"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800">
    <Grid>
        <Grid x:Name="EMark" Margin="0,0,0,157">
        </Grid>
        <Grid>
            <Button DockPanel.Dock="Left" Click="Button_Click"   HorizontalAlignment="Left" Width="37" Height="521" VerticalAlignment="Bottom">Parse</Button>
            <TextBlock x:Name="Error" DockPanel.Dock="Bottom" Foreground="Red" />
            <TextBox x:Name="Program" AcceptsReturn="True" AcceptsTab="True" TextChanged="Program_TextChanged" Height="521" Width="748" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,10,0" FontSize="18"/>
        </Grid>
    </Grid>

</Window>
